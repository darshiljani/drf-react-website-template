{
  "name": "vscode-react-spa-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "22.15.1"
  },
  "packageManager": "npm@10.9.2",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx,js,cjs --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write '**/*.{ts,tsx,js,cjs,html,htm,md,css,scss}'",
    "preview": "vite preview",
    "test": "vitest --passWithNoTests --run"
  },
  "dependencies": {
    "@types/node": "^20.11.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^3.1.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "typescript": "^5.8.3",
    "vite": "^5.4.19",
    "vitest": "^3.1.4"
  }
}
